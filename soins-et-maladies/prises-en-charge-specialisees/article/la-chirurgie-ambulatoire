$(document).ready(function (){
	if ($("html").attr('class').indexOf('pauvrete') > -1) {
		$('img.puce').each(function(){
			$(this).attr("src", "plugins/site/themes/mass_theme_sante/v1/puce-pauvrete.gif");
		});
	}
	$("*").each(function(){
		if($(this).css('font-family').match(/notosans-bold/) && $(this).css('font-weight') > 600) {
			$(this).css('font-weight','normal');
		}
	});

	function no_accent(my_string) {
		// tableau accents
		var pattern_accent = new Array(/À/g, /Á/g, /Â/g, /Ã/g, /Ä/g, /Å/g, /Æ/g, /Ç/g, /È/g, /É/g, /Ê/g, /Ë/g,
			/Ì/g, /Í/g, /Î/g, /Ï/g, /Ð/g, /Ñ/g, /Ò/g, /Ó/g, /Ô/g, /Õ/g, /Ö/g, /Ø/g, /Ù/g, /Ú/g, /Û/g, /Ü/g, /Ý/g,
			/Þ/g, /ß/g, /à/g, /á/g, /â/g, /ã/g, /ä/g, /å/g, /æ/g, /ç/g, /è/g, /é/g, /ê/g, /ë/g, /ì/g, /í/g, /î/g,
			/ï/g, /ð/g, /ñ/g, /ò/g, /ó/g, /ô/g, /õ/g, /ö/g, /ø/g, /ù/g, /ú/g, /û/g, /ü/g, /ý/g, /ý/g, /þ/g, /ÿ/g);

		// tableau sans accents
		var pattern_replace_accent = new Array("A","A","A","A","A","A","A","C","E","E","E","E",
			"I","I","I","I","D","N","O","O","O","O","O","O","U","U","U","U","Y",
			"b","s","a","a","a","a","a","a","a","c","e","e","e","e","i","i","i",
			"i","d","n","o","o","o","o","o","o","u","u","u","u","y","y","b","y");

		//pour chaque caractere si accentué le remplacer par un non accentué
		for (var i=0;i<pattern_accent.length;i++) {
			my_string = my_string.replace(pattern_accent[i],pattern_replace_accent[i]);
		}
		return my_string;
	}
	var slug = function(str) {
		var $slug = '';
		var trimmed = $.trim(str);
		$slug = trimmed.replace(/[^a-z0-9-]/gi, '_').
		replace(/_+/g, '_').
		replace(/^_|_$/g, '').
		replace(/_nouvelle_fenetre$/g, '');
		return $slug.toLowerCase();
	};

	
		$('a[href$="IMG/pdf/Flyer_SSA_2015_-_A4_-_Chirurgie_ambulatoire.pdf"]:not([onclick])').click(function() {
			return ATTag.click.send({elem:this, name:'flyer_de_presentation', level2:15, type:'download'});
		});
	
		$('a[href$="IMG/pdf/Chirurgie_ambulatoire_-_cahier_des_charges_appel_a_projets.pdf"]:not([onclick])').click(function() {
			return ATTag.click.send({elem:this, name:'le_cahier_des_charges_de_l_appel_a_projets', level2:15, type:'download'});
		});
	
		$('a[href$="IMG/doc/Chirurgie_ambulatoire_-_dossier_de_candidature.doc"]:not([onclick])').click(function() {
			return ATTag.click.send({elem:this, name:'le_dossier_de_candidature', level2:15, type:'download'});
		});
	
	$('a[href^="/fichiers/"]:not([onclick])').click(function(){
		var page_name = slug(no_accent($(this).attr('href')));
		// console.log(page_name);
		return ATTag.click.send({elem:this, name:page_name, level2:15, type:'download'});
	});
});

